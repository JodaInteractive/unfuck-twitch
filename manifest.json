{
    "name": "Unfuck Twitch",
    "description": "Fix Twitch's UX Mistakes",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "hello.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.twitch.tv/*"],
        "js": ["scripts/content.js"]
      }
    ]
  }